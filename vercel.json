{
    "version": 2,
    "builds": [
      {
        "src": "express.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/express",
        "methods": ["POST"],
        "dest": "/express.js"
      },
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ]
  }
  