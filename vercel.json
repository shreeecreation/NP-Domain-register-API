{
    "version" : 2,
    "builds" : [{
        "src" : "express.js",
        "methods": ["POST"],
        "use" :"@vercel/node"
    }],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "express.js"
        },
        {
            "src": "src/*",
            "use": "@vercel/node"
          }
    ]
}